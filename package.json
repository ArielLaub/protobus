    "test": "npm run build && npm run copy-test-proto && node ./dist/test/runner.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "version": "0.9.4"
