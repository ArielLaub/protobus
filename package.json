{
  "author": {
    "name": "Ariel Laub"
  },
  "bugs": {
    "url": "https://github.com/ArielLaub/protobus/issues"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "amqplib": "^0.10.4",
    "express": "^4.21.0",
    "protobufjs": "^7.4.0",
    "source-map-support": "^0.5.21"
  },
  "description": "The Protobus micro-services framework",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/amqplib": "^0.10.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "homepage": "https://github.com/ArielLaub/protobus#readme",
  "keywords": [
    "micro services",
    "bus",
    "amqp",
    "rabbitmq",
    "protobuf",
    "scalable",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "protobus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArielLaub/protobus.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint 'lib/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint 'lib/**/*.ts' 'test/**/*.ts' --fix",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "docker compose up -d --wait && jest --config jest.integration.config.js; docker compose down",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "docker:up": "docker compose up -d --wait",
    "docker:down": "docker compose down",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "version": "0.9.8"
}
